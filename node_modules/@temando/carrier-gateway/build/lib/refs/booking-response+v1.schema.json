{
  "id": "http://carrier-gateway.temando.com/schema/booking-response+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic booking response schema",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "createdAt": {
      "description": "The time the request was sent by the client"
    },
    "integrationId": {
      "description": "The identifier of the integration to talk to"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "booking": {
      "required": [
        "bookedAt",
        "bookingReference",
        "integrationServiceId",
        "serviceName",
        "shipment",
        "shippingTaxInclusiveCharge"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/booking+v2#"
        }
      ]
    }
  }
}
