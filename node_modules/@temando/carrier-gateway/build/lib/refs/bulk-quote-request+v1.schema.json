{
  "id": "http://carrier-gateway.temando.com/schema/bulk-quote-request+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic bulk quote request schema",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/credentials+v2"
    }
  ],
  "required": [
    "shipment"
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for this objects schema"
    },
    "createdAt": {
      "description": "The time the request was sent by the client"
    },
    "credentials": {
      "description": "The carrier credentials object"
    },
    "integrationId": {
      "description": "The identifier of the integration to talk to"
    },
    "shipment": {
      "description": "A shipment to quote for",
      "type": "object",
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/shipment+v2#"
        }
      ],
      "required": [
        "integrationServiceIds",
        "origin",
        "destination",
        "packages"
      ],
      "properties": {
        "integrationServiceIds": {
          "description": "An array of service identifiers to quote for",
          "type": "array",
          "items": [
            {
              "description": "A consistent identifier that represents a unique carrier service",
              "type": "string",
              "minLength": 1
            }
          ]
        }
      }
    }
  }
}
