{
  "id": "http://carrier-gateway.temando.com/schema/bulk-quote-response+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic bulk quote response schema",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/non-atomic+v1#"
    }
  ],
  "required": [
    "success",
    "failure"
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for this objects schema"
    },
    "createdAt": {
      "description": "The time the request was sent by the client"
    },
    "integrationId": {
      "description": "The identifier of the integration to talk to"
    },
    "success": {
      "description": "An array of quotes that were successfully computed",
      "type": "array",
      "items": [
        {
          "$ref": "#/definitions/eligibleQuote"
        }
      ]
    },
    "failure": {
      "description": "An array of errors that occured when trying to quote some particular services"
    }
  },
  "definitions": {
    "eligibleQuote": {
      "type": "object",
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/quote+v1#"
        }
      ],
      "required": [
        "shippingTaxInclusiveCharge",
        "serviceName",
        "carrierName",
        "pricedAt"
      ]
    }
  }
}
