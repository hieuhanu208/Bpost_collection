{
  "id": "http://carrier-gateway.temando.com/schema/cancel-request+v2#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic cancel booking request schema",
  "type": "object",
  "required": [
    "id",
    "integrationId",
    "booking",
    "configuration"
  ],
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "configuration": {
      "required": [
        "client"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/configuration+v1#"
        }
      ]
    },
    "id": {
      "description": "A unique identifier for this objects schema"
    },
    "createdAt": {
      "description": "The time the request was sent by the client"
    },
    "integrationId": {
      "description": "The identifier of the integration to talk to"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "booking": {
      "description": "The booking one wishes to cancel",
      "$ref": "http://carrier-gateway.temando.com/schema/generic/booking+v2#"
    }
  }
}
