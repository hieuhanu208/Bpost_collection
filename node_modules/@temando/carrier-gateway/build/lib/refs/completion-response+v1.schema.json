{
  "id": "http://carrier-gateway.temando.com/schema/completion-response+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic completion response schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "success",
    "failure",
    "references"
  ],
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/non-atomic+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for the schema of this response"
    },
    "integrationId": {
      "description": "The id of the integration responding"
    },
    "createdAt": {
      "description": "The time the integration generated this response"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "references": {
      "description": "An array of references for the transaction",
      "type": "array",
      "items": [
        {
          "description": "An association among a booking, manifest, and or pickup reference numbers",
          "type": "object",
          "properties": {
            "manifestReference": {
              "type": "string",
              "minLength": 1
            },
            "pickupReference": {
              "type": "string",
              "minLength": 1
            },
            "bookingReference": {
              "type": "string",
              "minLength": 1
            }
          }
        }
      ]
    },
    "documentation": {
      "description": "Documentation that has been generated as part of this transaction",
      "type": "array",
      "items": {
        "$ref": "http://carrier-gateway.temando.com/schema/generic/document+v1#"
      }
    },
    "success": {
      "description": "An array of successfully completed bookings identified by their reference number",
      "type": "array",
      "items": [
        {
          "description": "The reference number of a booking",
          "type": "object",
          "required": [
            "bookingReference"
          ],
          "properties": {
            "bookingReference": {
              "type": "string"
            },
            "process": {
              "description": "The process undertaken for completing this booking",
              "$ref": "http://carrier-gateway.temando.com/schema/generic/completion-process+v1#"
            }
          },
          "minLength": 1
        }
      ]
    },
    "failure": {
      "description": "An array of errors which occurred when trying to complete some bookings",
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "bookingReference": {
              "description": "An optional reference to the booking from which the error occured",
              "type": "string"
            }
          }
        }
      ]
    }
  }
}
