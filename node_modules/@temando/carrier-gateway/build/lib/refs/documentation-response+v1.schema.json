{
  "id": "http://carrier-gateway.temando.com/schema/documentation-response+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic documentation response schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "documentation"
  ],
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "integrationId": {
      "description": "The id of the integration you wish to talk to"
    },
    "createdAt": {
      "description": "The time the client sent this request to gateway"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "documentation": {
      "description": "Documentation that has been generated as part of this request",
      "type": "array",
      "items": {
        "allOf": [
          {
            "$ref": "http://carrier-gateway.temando.com/schema/generic/document+v1#"
          }
        ],
        "required": [
          "encoding",
          "mimeType",
          "type"
        ],
        "oneOf": [
          {
            "required": [
              "data",
              "encoding"
            ]
          },
          {
            "required": [
              "url"
            ]
          }
        ]
      }
    }
  }
}
