{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://carrier-gateway.temando.com/schema/generic/mime-type+v1#",
  "description": "A schema for an representing a mime type",
  "additionalProperties": false,
  "properties": {
    "mimeType": {
      "description": "A standard mime-type representation of the file format.",
      "type": "string",
      "enum": [
        "application/epl",
        "application/epl2",
        "application/zpl",
        "application/pdf",
        "image/png",
        "image/jpg",
        "image/gif",
        "image/bmp",
        "image/svg+xml",
        "text/plain",
        "text/csv",
        "text/xml",
        "text/json"
      ]
    }
  }
}
