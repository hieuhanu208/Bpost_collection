{
  "id": "http://carrier-gateway.temando.com/schema/paperless-delete-request+v2#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic paperless delete request schema",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "required": [
    "configuration"
  ],
  "properties": {
    "configuration": {
      "required": [
        "client"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/configuration+v1#"
        }
      ]
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "integrationId": {
      "description": "The id of the integration you wish to talk to"
    },
    "createdAt": {
      "description": "The time the client sent this request to gateway"
    },
    "booking": {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/booking+v2#"
    },
    "documentId": {
      "description": "The id of the document that you wish to remove from the booking",
      "type": "string"
    }
  }
}
