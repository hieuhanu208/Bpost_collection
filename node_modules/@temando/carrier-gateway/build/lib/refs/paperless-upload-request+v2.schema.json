{
  "id": "http://carrier-gateway.temando.com/schema/paperless-upload-request+v2#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic paperless upload request schema",
  "type": "object",
  "additionalProperties": false,
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "required": [
    "configuration"
  ],
  "properties": {
    "configuration": {
      "required": [
        "client"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/configuration+v1#"
        }
      ]
    },
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "integrationId": {
      "description": "The id of the integration you wish to talk to"
    },
    "createdAt": {
      "description": "The time the client sent this request to gateway"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "booking": {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/booking+v2#"
    },
    "document": {
      "description": "a document that you wish to link to the booking",
      "type": "object",
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/document+v1#"
        }
      ],
      "required": [
        "encoding",
        "mimeType",
        "type"
      ],
      "oneOf": [
        {
          "required": [
            "data"
          ]
        },
        {
          "required": [
            "url"
          ]
        }
      ]
    }
  }
}
