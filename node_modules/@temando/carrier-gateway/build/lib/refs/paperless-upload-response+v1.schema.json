{
  "id": "http://carrier-gateway.temando.com/schema/paperless-upload-response+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The paperless upload response schema",
  "type": "object",
  "required": [
    "documents"
  ],
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "documents": {
      "description": "An array of document upload information",
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "#/definitions/document"
      }
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    }
  },
  "definitions": {
    "document": {
      "type": "object",
      "description": "A description of an uploaded document",
      "properties": {
        "id": {
          "description": "A consistent identifier for the uploaded document",
          "type": "string",
          "minLength": 1
        }
      }
    }
  }
}
