{
  "id": "http://carrier-gateway.temando.com/schema/quote-request+v4#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic schema for quote requests",
  "type": "object",
  "additionalProperies": false,
  "required": [
    "id",
    "integrationId",
    "createdAt",
    "shipment"
  ],
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/credentials+v2"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/shipment+v2#"
    }
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "integrationId": {
      "description": "The id of the integration you wish to talk to"
    },
    "createdAt": {
      "description": "The time the client sent this request to gateway"
    },
    "credentials": {
      "description": "Carrier credentials to make the request with"
    },
    "shipment": {
      "required": [
        "integrationServiceId",
        "origin",
        "destination",
        "packages"
      ]
    }
  }
}
