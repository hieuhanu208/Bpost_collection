{
  "id": "http://carrier-gateway.temando.com/schema/quote-request+v5#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "The generic quote request schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "shipment",
    "configuration"
  ],
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/shipment+v2#"
    },
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/extensible+v1#"
    }
  ],
  "properties": {
    "configuration": {
      "required": [
        "client"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/configuration+v1#"
        }
      ]
    },
    "id": {
      "description": "A unique identifier for this objects schema"
    },
    "createdAt": {
      "description": "The time the request was sent by the client"
    },
    "integrationId": {
      "description": "The identifier of the integration to talk to"
    },
    "extensions": {
      "description": "Custom extensions to this schema"
    },
    "shipment": {
      "description": "A shipment to quote for",
      "type": "object",
      "required": [
        "integrationServiceIds",
        "origin",
        "destination",
        "packages"
      ]
    }
  }
}
