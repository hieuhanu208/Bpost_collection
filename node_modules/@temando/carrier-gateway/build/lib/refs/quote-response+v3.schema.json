{
  "id": "http://carrier-gateway.temando.com/schema/quote-response+v3#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A generic response for a quote request",
  "type": "object",
  "allOf": [
    {
      "$ref": "http://carrier-gateway.temando.com/schema/generic/identifiable+v1#"
    }
  ],
  "additionalProperties": false,
  "required": [
    "quote"
  ],
  "properties": {
    "id": {
      "description": "A unique identifier for the schema of this request"
    },
    "integrationId": {
      "description": "The id of the integration you wish to talk to"
    },
    "createdAt": {
      "description": "The time the client sent this request to gateway"
    },
    "quote": {
      "type": "object",
      "required": [
        "shippingTaxInclusiveCharge",
        "pricedAt",
        "carrierName",
        "serviceName"
      ],
      "allOf": [
        {
          "$ref": "http://carrier-gateway.temando.com/schema/generic/quote+v1#"
        }
      ]
    }
  }
}
