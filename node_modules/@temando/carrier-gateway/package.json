{
  "name": "@temando/carrier-gateway",
  "version": "2.15.0",
  "description": "A serverless project that mediates communication to individual carriers via integrations",
  "author": "Temando Pty Ltd",
  "license": "SEE LICENSE IN LICENCE",
  "repository": {
    "type": "git",
    "url": "git://src.temando.io:carrier-framework/carrier-gateway.git"
  },
  "scripts": {
    "clean": "rm -rf build doc",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --recursive --timeout 15000 test",
    "lint": "eslint index.js lib/**/**/*.js lib/*.js",
    "build": "npm run clean && babel index.js --out-dir build --copy-files && babel lib --out-dir build/lib --copy-files && npm test",
    "document": "esdoc -c .esdoc.json"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "@temando/instrument": "^1.3.3",
    "ajv": "^4.11.7",
    "aws-sdk": "~2.6",
    "bluebird": "^3.4.6",
    "dotenv": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-module-method": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.5.2",
    "chai": "^4.0.2",
    "chai-ajv-json-schema": "^1.0.0",
    "dirty-chai": "^1.2.2",
    "esdoc": "^0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "mocha": "^3.4.2"
  }
}
