{
  "name": "@temando/generic-schema",
  "version": "0.19.0",
  "description": "A collection of generic schemas used throughout the Temando service eco-system",
  "configuration": {
    "host": "https://schema.temando.io",
    "path": "/generic-schema/v0"
  },
  "scripts": {
    "test": "jest --config .jestrc dist",
    "build": "source ./scripts/package",
    "release": "cd dist && npm publish",
    "lint": "tslint 'src/**/*.ts' --type-check --project tsconfig.json",
    "docs": "typedoc --out ./dist/docs --target es6 --readme README.md",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:schema/generic-schema.git"
  },
  "keywords": [
    "json-schema"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jdw/jst": "^1.0.0-beta.10",
    "@types/chai": "^3.5.0",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.5",
    "ajv": "^4.11.5",
    "chai": "^3.5.0",
    "changelog-verify": "^1.0.4",
    "jest": "^19.0.2",
    "swagger-tools": "^0.10.1",
    "ts-jest": "^19.0.8",
    "ts-node": "^3.0.2",
    "tslint": "^5.5.0",
    "tslint-config-temando": "^1.1.4",
    "tslint-eslint-rules": "^4.0.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.2.2",
    "version-changelog": "^2.1.0"
  }
}
