{
  "id": "https://app.temando.com/schemas/generic/form/controls/info-form-control+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Displays info text e.g. terms and conditions.",
  "type": "object",
  "required": ["type"],
  "properties": {
    "type": {
      "type": "string",
      "enum": ["infoText"]
    },
    "dataSource": {
      "$ref": "https://app.temando.com/schemas/generic/form/data-source+v1#"
    },
    "scrollable": {
      "type": "boolean"
    },
    "printable": {
      "type": "boolean"
    }
  }
}
