  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["maximum", "messages"],
  "properties": {
    "maximum": { "type": "integer" },
    "messages": { "$ref": "#/definitions/errorMessages" }
  },
  "definitions": {
    "errorMessages": {
      "type": "object",
      "required": ["tooLong"],
      "properties": {
        "tooLong": {
          "$ref": "https://app.temando.com/schema/generic/message+v1#"
        }
      }
    }
  }
}