{
  "id": "https://app.temando.com/schemas/generic/value+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "oneOf": [
    {
      "type": "object",
      "required": ["static"],
      "additionalProperties": false,
      "properties": {
        "static": {
          "description": "A static value",
          "type": ["string", "array", "number", "boolean", "null"]
        }
      }
    },
    {
      "type": "object",
      "required": ["generate"],
      "additionalProperties": false,
      "properties": {
        "generate": {
          "description": "A generated value",
          "type": "string",
          "enum": [
            "dateNow",
            "uuid"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": ["stateRef"],
      "additionalProperties": false,
      "properties": {
        "stateRef": {
          "description": "JSON Pointer (RFC 6901). The path within state to access a value",
          "type": "string"
        },
        "transforms": {
          "type": "array",
          "items": {
            "$ref": "https://app.temando.com/schemas/generic/transform/transform+v1#"
          },
          "minItems": 1
        }
      }
    }
  ]
}
