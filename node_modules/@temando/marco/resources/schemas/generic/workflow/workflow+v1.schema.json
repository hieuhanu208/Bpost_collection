{
  "id": "https://app.temando.com/schema/generic/workflow/workflow+v1#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": ["steps"],
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "$ref": "https://app.temando.com/schema/generic/message+v1#"
    },
    "steps": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "https://app.temando.com/schema/generic/workflow/step+v1#"
      }
    }
  }
}
