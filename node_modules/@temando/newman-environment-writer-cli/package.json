{
  "name": "@temando/newman-environment-writer-cli",
  "version": "0.2.0",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.3.2"
  },
  "description": "A CLI interface for writing Newman environment files.",
  "author": "brendan.abbott@temando.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:service-registry/newman-environment-writer.git"
  },
  "scripts": {
    "build": "$(npm bin)/babel bin --out-dir build",
    "clean": "rm -Rf build",
    "deploy": "npm run clean && npm run build && npm link",
    "lint": "$(npm bin)/eslint bin",
    "preversion": "npm run lint && npm run clean && npm run build"
  },
  "dependencies": {
    "@temando/service-registry-lib": "latest",
    "aws-sdk": "^2.59.0",
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-latest-node4": "^1.0.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint": "^3.11.1"
  },
  "bin": {
    "temando-newman": "build/temando-newman.js"
  },
  "main": "build",
  "files": [
    "build"
  ]
}
