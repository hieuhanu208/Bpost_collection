{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "func-names": 0,
    "no-unused-vars": 0,
    "id-length": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-extend-native": 0,
    "no-self-compare": 0,
    "arrow-body-style": 0
  }
}
