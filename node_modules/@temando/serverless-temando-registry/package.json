{
  "name": "@temando/serverless-temando-registry",
  "version": "0.4.1",
  "description": "A Serverless plugin that updates a Service Registry with deployment information for the service being deployed.",
  "main": "dist/",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:service-registry/serverless-temando-service-registry-plugin.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "$(npm bin)/eslint src test",
    "clean": "rm -Rf ./dist",
    "build": "babel $BABEL_OPTIONS --source-maps inline ./src --out-dir ./dist --copy-files",
    "dev": "BABEL_OPTIONS=--watch npm run build",
    "preversion": "npm run clean && npm run build && npm run lint && npm run test"
  },
  "author": "Abilio Henrique <abilio.henrique@temando.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-module-method": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^19.0.2"
  },
  "dependencies": {
    "@temando/service-registry-lib": "^0.7.1",
    "@types/fs-promise": "^1.0.3",
    "aws-sdk": "^2.80.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "chalk": "^2.0.1",
    "fs-promise": "^2.0.3",
    "js-yaml": "^3.8.4",
    "lutils": "^1.2.2",
    "trans-interpolator": "^1.0.4"
  }
}
