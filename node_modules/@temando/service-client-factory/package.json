{
  "name": "@temando/service-client-factory",
  "version": "2.4.1",
  "description": "The Service Client Factory creates clients that can be used to communicate with other services in the Temando Service Registry.",
  "scripts": {
    "test": "yarn test:unit",
    "test:coverage": "yarn test:unit -- --coverage --coverageReporters text text-summary",
    "test:dist": "yarn test:unit:dist",
    "test:unit": "jest --verbose -i src",
    "test:unit:dist": "jest -c '{}' dist",
    "lint": "tslint -p tsconfig.json -c tslint.json --type-check 'src/**/*.ts'",
    "clean": "rm -Rf dist",
    "build": "./scripts/build",
    "build:docs": "typedoc --options typedoc.json",
    "build:link": "scripts/build && cd dist && yarn link",
    "preversion": "yarn lint && yarn build && yarn test:dist && changelog-verify CHANGELOG.md",
    "version": "yarn build && version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:service-registry/service-client-factory.git"
  },
  "keywords": [
    "library"
  ],
  "author": "Abilio Henrique <abilio.henrique@temando.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@temando/errors": "^0.2.0",
    "@temando/service-registry-lib": "^0.7.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/joi": "^10.4.1",
    "aws-sdk": "^2.80.0",
    "aws4": "^1.6.0",
    "bluebird": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^10.6.0",
    "lutils": "^2.4.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/js-yaml": "^3.9.1",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.24",
    "aws-sdk-mock": "^1.7.0",
    "changelog-verify": "^1.0.4",
    "jest": "^20.0.4",
    "js-yaml": "^3.9.1",
    "nock": "^9.0.14",
    "ts-jest": "^20.0.10",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-temando": "^1.2.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2",
    "version-changelog": "^2.1.0"
  },
  "optionalDependencies": {}
}
