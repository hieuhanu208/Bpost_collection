{
  "name": "@temando/service-library",
  "description": "The library for all your micro service needs.",
  "version": "7.1.1",
  "license": "SEE LICENSE IN LICENCE.md",
  "keywords": [
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:service-library/temando-service-library.git"
  },
  "scripts": {
    "test": "jest src",
    "coverage": "jest src --coverage",
    "test:dist": "jest dist",
    "lint": "tslint --project tsconfig.json --type-check",
    "release": "cd dist && npm publish",
    "test:integration": "jest --testMatch **/src/**/*int-test*",
    "build": "bash -v -e scripts/build",
    "build:link": "yarn build && cd dist && yarn link",
    "preversion": "yarn lint && yarn build && yarn test:dist && changelog-verify CHANGELOG.md",
    "docs": "typedoc --includeDeclarations --excludeExternals --out ./docs/typedoc --target es6 --readme README.md --excludePrivate",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"
  },
  "devDependencies": {
    "@temando/service-client-factory": "^2.3.2",
    "@temando/storage-library": "^2.3.0",
    "@types/jest": "^20.0.8",
    "@types/webpack": "^3.0.10",
    "aws-sdk-mock": "^1.7.0",
    "changelog-verify": "^1.0.4",
    "jest": "^21.0.1",
    "ts-jest": "^21.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "tslint-config-temando": "^1.5.0",
    "typedoc": "^0.8.0",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.5.2",
    "version-changelog": "^2.1.0",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "@temando/errors": "^0.2.0",
    "@temando/generic-schema": "^0.19.0",
    "@temando/instrument": "^4.1.1",
    "@types/bluebird": "^3.5.8",
    "@types/chalk": "^0.4.31",
    "@types/qs": "^6.5.0",
    "@types/uuid": "^3.4.0",
    "ajv": "^4.11.2",
    "aws-sdk": "^2.103.0",
    "bluebird": "^3.5.0",
    "chalk": "^2.1.0",
    "json-pointer": "^0.6.0",
    "lodash.get": "^4.4.2",
    "lutils": "^2.4.0",
    "qs": "^6.5.0",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "@temando/service-client-factory": "^2.3.2",
    "@temando/storage-library": "^2.3.0"
  }
}
