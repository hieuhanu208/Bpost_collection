{
  "name": "@temando/service-registry-cli",
  "version": "0.4.1",
  "preferGlobal": true,
  "engines": {
    "node": ">=4.3.2"
  },
  "description": "A CLI interface for the Temando Service Registry",
  "author": "paul.croker@temando.com",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "git@src.temando.io:service-registry/service-registry-cli.git"
  },
  "keywords": [
    "library"
  ],
  "scripts": {
    "lint": "$(npm bin)/eslint bin",
    "clean": "rm -Rf build",
    "build": "$(npm bin)/babel bin --out-dir build",
    "deploy": "npm run clean && npm run build && npm link",
    "preversion": "npm run build && npm run lint"
  },
  "dependencies": {
    "@temando/service-registry-lib": "^0.7.1",
    "aws-sdk": "2.6.15",
    "commander": "^2.9.0",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.11.6",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^10.0.0",
    "eslint-plugin-import": "^2.2.0"
  },
  "bin": {
    "registry": "build/registry.js",
    "registry-kv": "build/registry-kv.js",
    "registry-catalog": "build/registry-catalog.js",
    "registry-service": "build/registry-service.js"
  },
  "main": "build",
  "files": [
    "build"
  ]
}
