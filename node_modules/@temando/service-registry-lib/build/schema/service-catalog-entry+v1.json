  "id": "http://service-registry.temando.com/schema/service-catalog-entry+v1#",
  "type": "object",
  "required": ["definition", "deployments"],
  "properties": {
    "definition": {
      "$ref": "http://service-registry.temando.com/schema/service-definition+v1#"
